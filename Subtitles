   "cell_type": "code",
   "execution_count": 41,
   "id": "e3b88cda-697d-41a3-a790-49efd9d9d19e",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "def store_vtt_subtitles(vtt_file, subtitles):\n",
    "    with open(vtt_file, \"w\", encoding=\"utf-8\") as f:\n",
    "        f.write(\"WEBVTT\\n\\n\")\n",
    "        for subtitle in subtitles:\n",
    "            f.write(subtitle[\"timestamp\"] + \"\\n\")\n",
    "            f.write(subtitle[\"french_text\"] + \"\\n\\n\")\n",
    "        f.close()\n",
    "\n",
    "store_vtt_subtitles('./Movies/Aquí No Hay Quien Viva 1/A1_t00_fr_save.vtt', saved_subtitles)"
   ]
  },
