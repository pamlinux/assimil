    "with open(\"subtitles.save\", \"r\", encoding=\"utf-8\") as f:\n",
    "    contenu = f.read()\n",
    "\n",
    "# Si votre variable était un dictionnaire ou une liste simple représentée par repr()\n",
    "try:\n",
    "    saved_subtitles = eval(contenu)\n",
    "    print(saved_subtitles)\n",
    "except (SyntaxError, NameError):\n",
    "    print(\"Impossible de réévaluer directement le contenu.\")\n"
   ]
  },
